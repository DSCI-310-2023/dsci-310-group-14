visualize_knn <- function(df, neighbors, acc, title){
plot <- ggplot(df, aes(x = neighbors, y = acc)) +
geom_line() +
geom_point() +
labs(title = title)+
xlab("Number of K") +
ylab("Accuracy") + theme(text = element_text(size = 18))# +
#ggtitle(title)
return (plot)
}
